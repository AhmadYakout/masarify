{
  "name": "masrmoney",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "doctor": "node scripts/doctor.mjs development",
    "doctor:mock": "node scripts/doctor.mjs mock",
    "doctor:staging": "node scripts/doctor.mjs staging",
    "dev": "npm run dev:frontend",
    "dev:frontend": "vite --mode development",
    "dev:frontend:mock": "vite --mode mock",
    "dev:frontend:staging": "vite --mode staging",
    "dev:backend": "npm run dev --prefix backend",
    "dev:backend:mock": "npm run dev:mock --prefix backend",
    "dev:backend:staging": "npm run dev:staging --prefix backend",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:all:mock": "concurrently \"npm run dev:frontend:mock\" \"npm run dev:backend:mock\"",
    "dev:all:staging": "concurrently \"npm run dev:frontend:staging\" \"npm run dev:backend:staging\"",
    "build": "npm run build:frontend",
    "build:frontend": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "preview": "npm run preview:frontend",
    "preview:frontend": "vite preview --mode production",
    "preview:staging": "vite preview --mode staging",
    "typecheck": "tsc --noEmit",
    "typecheck:backend": "npm run typecheck --prefix backend",
    "start:backend:prod": "npm run start --prefix backend",
    "start:frontend:prod": "npm run preview:frontend",
    "mobile:sync": "npm run build && npx cap sync",
    "mobile:android:add": "npx cap add android",
    "mobile:android:sync": "npm run build && npx cap sync android"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^7.0.3",
    "@capacitor/android": "^7.3.0",
    "@capacitor/core": "^7.3.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.3.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
